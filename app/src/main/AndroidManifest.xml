<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.techtown.havit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Samplehavit">
        <activity
            android:name=".MainActivity"
            android:label="Samplehavit"
            android:theme="@style/Theme.Samplehavit.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DisposableActivity"></activity>
        <activity android:name=".DisposableDetailActivity"></activity>
        <activity android:name=".DisposableDetailActivity2"></activity>
        <activity android:name=".InformationActivity"></activity>
        <activity android:name=".BasketActivity"></activity>
    </application>

</manifest>